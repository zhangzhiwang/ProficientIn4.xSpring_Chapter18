<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.asiainfo.dao.IUserDao">
	<insert id="save" parameterType="User">
		insert into t_user (user_name,password,credits,last_visit,last_ip) values (#{userName},#{password},#{credits},#{lastVisit},#{lastIp})
	</insert>
</mapper>